<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="550.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="foodintake.viewfoodintake.DisplayFoodIntakeViewController">
   <children>
      <Button fx:id="deleteButton" layoutX="665.0" layoutY="477.0" mnemonicParsing="false" onAction="#handleDelete" text="Delete" />
      <Button fx:id="backButton" layoutX="44.0" layoutY="26.0" mnemonicParsing="false" text="Back" />
      <TableView fx:id="foodIntakeTable" layoutX="305.0" layoutY="156.0" prefHeight="265.0" prefWidth="333.0">
        <columns>
          <TableColumn fx:id="idColumn" prefWidth="75.0" text="ID" />
          <TableColumn fx:id="foodNameColumn" prefWidth="99.0" text="Food Name" />
            <TableColumn fx:id="caloriesColumn" prefWidth="68.0" text="Calories" />
            <TableColumn fx:id="dateColumn" prefWidth="113.0" text="Date" />
        </columns>
      </TableView>
      <Button fx:id="editButton" layoutX="575.0" layoutY="477.0" mnemonicParsing="false" onAction="#handleEditEntry" text="Edit" />
      <ImageView fitHeight="75.0" fitWidth="83.0" layoutX="367.0" layoutY="15.0">
         <image>
            <Image url="@../Icon_FoodTracker.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="40.0" fitWidth="200.0" layoutX="142.0" layoutY="32.0">
         <image>
            <Image url="@../Logo_myFitnessFriend.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
